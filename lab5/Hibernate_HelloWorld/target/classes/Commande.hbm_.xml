<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.Commande" table="orders">
		<id name="ordno" column="ordno" type="integer" />
		<property name="mois" column="month" type="string" />
		<property name="qty" column="qty" type="integer" />
		<property name="dollars" column="dollars" type="integer" />

		<many-to-one name="c" class="Model.Client" fetch="join">
			<column name="cid" />
		</many-to-one>

	</class>
</hibernate-mapping>